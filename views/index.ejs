<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/style.css" />
    </head>
    <body>
        <h1><%= title %></h1>
        <h2><a href="./new">Add New Message</a></h2>
        <% messages.forEach(function(message) { %>
        <p><%= message.user %></p>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
        <% }); %>
    </body>
</html>
